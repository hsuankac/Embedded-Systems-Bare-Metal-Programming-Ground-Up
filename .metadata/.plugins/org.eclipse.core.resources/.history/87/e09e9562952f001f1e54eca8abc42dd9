#include "adxl345.h"


char data;


void adxl_read_address(uint8_t reg)
{
	I2C_byteRead(DEVICE_ADDR, ref, &data);
}

void adxl_write_address(uint8_t reg, char value)
{
	char data[1];
	data[0] = value;
	I2C1_burstWrite(DEVICE_ADDR, reg, 1, data);
}
